{
  "name": "slackbot-v2",
  "version": "1.0.0",
  "description": "Another test",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-dev": "nodemon ./server-build/index.js",
    "tsc-linux": "/home/chau/.npm-global/lib/node_modules/typescript/bin/tsc",
    "start-dev-linux": "/home/chau/.npm-global/lib/node_modules/nodemon/bin/nodemon.js ./server-build/index.js",
    "heroku-prebuild": "export NPM_CONFIG_PRODUCTION=false; export NODE_ENV=development; npm install --only=dev --dev",
    "heroku-postbuild": "export NPM_CONFIG_PRODUCTION=true; export NODE_ENV=production;",
    "routes-gen": "tsoa routes",
    "swagger-gen": "tsoa swagger",
    "clean": "rimraf ./build && rimraf ./src/routes.ts && rimraf ./src/swagger.json",
    "tsoa-gen": "npm run routes-gen && npm run swagger-gen",
    "swagger-copy": "cp ./server-src/swagger.json ./server-build/swagger.json",
    "start-gen-dev": "npm run start-gen && nodemon ./build/index.js",
    "start-gen": "npm run clean && npm run tsoa-gen && tsc && npm run swagger-copy",
    "start": "tsc && cp ./server-src/newrelic.js ./server-build/newrelic.js && npm run swagger-copy && node ./server-build/index.js"
  },
